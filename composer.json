{
    "name": "mikeweb85/cakephp-sqlsrv",
    "description": "Sqlsrv plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-sqlsrv": "*",
        "ext-pdo_sqlsrv": "*",
        "cakephp/cakephp": "^4 || ^5",
        "mikeweb85/cakephp-odbc": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "MikeWeb\\CakeSqlsrv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MikeWeb\\CakeSqlsrv\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
